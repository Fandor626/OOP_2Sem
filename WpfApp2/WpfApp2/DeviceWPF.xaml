        <DatePicker HorizontalAlignment="Center" Margin="0,116,0,0" VerticalAlignment="Top" Width="234"/>
        <Button x:Name="Cancel" Content="Cancel" HorizontalAlignment="Center" Margin="0,376,0,0" VerticalAlignment="Top" Height="36" Width="114" Click="Cancel_Click"/>
        <Button x:Name="Save" Content="Save" HorizontalAlignment="Center" Margin="0,335,0,0" VerticalAlignment="Top" Height="36" Width="114" Click="Save_Click"/>
        <TextBox x:Name="TextBox" HorizontalAlignment="Left" Margin="63,157,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="181" Height="30"/>
        <Label x:Name="num" Content="Numb" HorizontalAlignment="Left" Margin="10,159,0,0" VerticalAlignment="Top" Height="28" Width="48"/>

    </Grid>
</Window>
