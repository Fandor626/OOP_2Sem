        <Button x:Name="Change" Content="Change" HorizontalAlignment="Left" Margin="177,21,0,0" VerticalAlignment="Top" Height="29" Width="53" Click="Change_Click"/>
        <Button x:Name="Create" Content="Create" HorizontalAlignment="Left" Margin="177,65,0,0" VerticalAlignment="Top" Height="29" Width="53" Click="Create_Click"/>
